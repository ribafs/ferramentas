			"<!-- Formulário de exemplo-->",
			"<h2>${1:Cadastro de Clientes}</h2>",
			"<form method=\"${2:POST}\" action = \"${3:cadastro.php}\">",
			"<table class=\"table\">",
			"<tr><td>${4:Nome}</td><td><input type=\"text\" name=\"${5:nome}\" required autofocus></td></tr>",
			"<tr><td>${6:E-mail}</td><td><input type=\"${7:email}\" name=\"${8:email}\"></td></tr>",
			"<tr><td></td><td><input type=\"submit\" value=\"Enviar\"></td></tr>",
			"</table>",
			"</form>"
		],
		"description": "Formulário de Exemplo"
	}, 
